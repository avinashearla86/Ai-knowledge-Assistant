Set-Content .gitignore @"
# Logs
logs
*.log
npm-debug.log*
yarn-debug.log*
yarn-error.log*
pnpm-debug.log*
lerna-debug.log*

# Dependency directories
node_modules/
dist/
dist-ssr/
*.local

# Editor directories
.vscode/*
!.vscode/extensions.json
.idea
.DS_Store
*.suo
*.ntvs*
*.njsproj
*.sln
*.sw?

# Python virtual environment
backend/venv/
venv/
env/

# Environment variables (Sensitive Data)
backend/.env
.env
frontend/.env

# Python cache
__pycache__/
*.pyc
*.pyo
"@